{
  "name": "pokeapprove",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Insert a random Pokemon gif into your Github reviews",
  "homepage_url": "http://boxxen.org",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "run_at": "document_end" 
    }
  ]
}
